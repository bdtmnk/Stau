		bool isfakeTauFromJet = genTauMatched<0.5 && !isfakeTauFromPromptLepton ;
		bool isfakeTauFromHadron = matchedTauToMuHadronDec > 0.5 || matchedTauToElHadronDec > 0.5;
		bool isfakeTauFromGluon = matchedTauToGluon>0.5 ;
		bool isfakeTauFromLFQ = isfakeTauFromJet && matchedTauToLFQ>0.5 ;
		bool isfakeTauFromHFQ = isfakeTauFromJet && matchedTauToHFQ>0.5 ;
		
		//if (isfakeTauFromHadron) isfakeTauFromJet = true;

		bool isfakeTauFromMu =  ( genTauMatched < 0.5 && matchedTauToPromptMu > 0.5 && !isfakeTauFromJet && !isData) ;
		bool isfakeTauFromEl =  ( genTauMatched < 0.5 && matchedTauToPromptEl > 0.5 && !isfakeTauFromJet && !isData) ;
	

		bool isfakeMuFromGluon =  ( genLeptonMatchedGluon >0.5 && !isMuPromptLepton && !isData) ;
		bool isfakeMuFromHFQ =  (  genLeptonMatchedHFQ >0.5 && !isMuPromptLepton && !isData) ;
		bool isfakeMuFromLFQ =  (  genLeptonMatchedLFQ >0.5 && !isMuPromptLepton && !isData) ;

		bool isLeptonFromLepton = (genLeptonMatched >0.5 && !isData);

		bool isfakeMuFromHadron = ( (genMuMatchedHadrDecay > 0.5 || genTauMatchedHadrDecay > 0.5 || genElMatchedHadrDecay >0.5)&&  !isMuPromptLepton && !isData);

		//category 1 : gen tau gen lepton		
		if (isRealTau) {

			if (isMuFromTau) category = 1;
			if (isMuPromptLepton) category = 2;
			if (isfakeMuFromGluon) category = 3;
			if (isfakeMuFromHFQ) category = 4;
			if (isfakeMuFromLFQ) category = 5;
//cout<<" It is really a real Tau !! "<<isRealTau<<"  "<<isMuFromTau<<" isMuPromptLepton "<<isMuPromptLepton<<" category "<<category<<endl;

		}

int cJ = 0;
		if (!isRealTau) {

	if (!isfakeTauFromGluon){
		if (isfakeTauFromJet && isMuFromTau) category = 6;
		if (isfakeTauFromJet && isMuPromptLepton) category = 7;
		if (isfakeTauFromJet && isfakeMuFromGluon) category = 8;
		if (isfakeTauFromJet && isfakeMuFromHFQ) category = 9;
		if (isfakeTauFromJet && isfakeMuFromLFQ) category = 10;
		if (isfakeTauFromJet && isfakeMuFromHadron)  category = 11;
		if (isfakeTauFromJet && !isMuFromTau && !isMuPromptLepton && !isfakeMuFromGluon && !isfakeMuFromHFQ && !isfakeMuFromLFQ && !isfakeMuFromHadron)  category = 12;
		}


